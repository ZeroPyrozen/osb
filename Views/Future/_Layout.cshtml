<body>
<div class="player_position_top_center">
    <div class="player_container">
        <div class="play_and_current_time">
            <button class="play_button">
                <img class="play_arrow_icon" src="/staticV2/svg/play_arrow.svg"/>
            </button>
            <div class="timer">
                <span id="current_time">0:00</span>
                <span class="separator">/</span>
                <span class="total_time">0:10</span>
            </div>
        </div>
        <input type="range" class="time_slider" min="0" max="10000" value="0"/>
    </div>
    <div class="volume_control">
        <div class="volume_down_icon">
            <img class="vector_icon" src="~/staticV2/svg/volume_down.svg" alt=""/>
            <span class="volume_percentage">100%</span>
        </div>
        <input type="range" class="volume_slider" min="0" max="100" value="100"/>
    </div>
</div>
