<script>
    function openPopover(popoverId) {
      const popover = document.getElementById(popoverId);
      if (popover) {
        popover.showModal();
        document.body.classList.add('no-scroll');
      }
    }

    function closePopover(popoverId) {
      const popover = document.getElementById(popoverId);
      if (popover) {
        popover.close();
        document.body.classList.remove('no-scroll');
      }
    }
  </script>